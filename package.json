{
  "name": "boilerplate-react-redux",
  "version": "0.2.0",
  "description": "React Redux using express",
  "main": "server/index.js",
  "scripts": {
    "knex": "knex --knexfile server/db/knexfile.js",
    "client": "npm run webpack",
    "server": "node server",
    "dev:client": "npm run webpack -- --watch",
    "dev:server": "nodemon server",
    "webpack": "webpack --config client/webpack.config.js",
    "start": "run-s client server",
    "dev": "run-p dev:client dev:server",
    "lint": "eslint . --ext .js,.jsx",
    "test": "jest"
  },
  "author": "EDA",
  "license": "ISC",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-runtime"
    ]
  },
  "eslintConfig": {
    "extends": "eda",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.8.0",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "express-jwt": "^6.1.0",
    "express-jwt-authz": "^2.4.1",
    "jest": "^26.6.3",
    "jwks-rsa": "^2.0.4",
    "knex": "^0.95.11",
    "nock": "^13.1.3",
    "react-router-dom": "^5.3.0",
    "redux-thunk": "^2.3.0",
    "sqlite3": "^5.0.2",
    "superagent": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^13.2.1",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.3.0",
    "dotenv": "^10.0.0",
    "eslint": "7.9.0",
    "eslint-config-eda": "0.1.4",
    "nodemon": "^2.0.7",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.0",
    "supertest": "^6.1.6",
    "webpack": "^5.33.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cristophergc/oil-collection.git"
  },
  "bugs": {
    "url": "https://github.com/cristophergc/oil-collection/issues"
  },
  "homepage": "https://github.com/cristophergc/oil-collection#readme"
}
